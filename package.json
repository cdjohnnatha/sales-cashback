{
  "name": "sales-cashback",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node -r dotenv/config ./api/config/bin/www",
    "dev": "nodemon -r dotenv/config ./api/config/bin/www",
    "test": "./node_modules/mocha/bin/mocha ./test/**/* --exit",
    "coverage": "./node_modules/nyc/bin/nyc.js --reporter=text-lcov > coverage.lcov && codecov ./node_modules/mocha/bin/mocha ./test/api/v1/**.*.js --exit",
    "test-coverage": "./node_modules/nyc/bin/nyc.js report --reporter=html --reporter=text ./node_modules/mocha/bin/mocha ./test/api/v1/**.*.js --exit",
    "swagger-json": "./node_modules/gulp/bin/gulp.js swagger",
    "build-database": "./node_modules/sequelize-cli/lib/sequelize db:create && ./node_modules/sequelize-cli/lib/sequelize db:migrate && ./node_modules/sequelize-cli/lib/sequelize db:seed:all",
    "database-migration": "./node_modules/sequelize-cli/lib/sequelize db:migrate",
    "database-drop": "./node_modules/sequelize-cli/lib/sequelize db:drop"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "bcrypt": "^4.0.1",
    "bottleneck": "^2.19.5",
    "concat-files": "^0.1.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "express": "~4.16.1",
    "gulp": "^4.0.2",
    "houston-errors": "^1.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.3",
    "pg": "^8.0.2",
    "sequelize": "^5.21.6",
    "simple-object-handler": "0.0.1",
    "winston": "^3.2.1",
    "yup": "^0.28.3"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "codecov": "^3.6.5",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "factory-girl": "^5.0.4",
    "faker": "^4.1.0",
    "mocha": "^10.2.0",
    "mocha-junit-reporter": "^1.23.3",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "sequelize-cli": "^5.5.1"
  }
}
